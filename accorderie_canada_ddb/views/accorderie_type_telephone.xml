<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="accorderie_type_telephone_view_form" model="ir.ui.view">
        <field name="name">accorderie_type_telephone_form</field>
        <field name="model">accorderie.type.telephone</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="nom" />
                    </group>
                    <group>
                        <field name="membre" />
                    </group>
                    <group>
                        <field name="membre_2_ids" />
                    </group>
                    <group>
                        <field name="membre_ids" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="accorderie_type_telephone_view_graph" model="ir.ui.view">
        <field name="name">accorderie_type_telephone_graph</field>
        <field name="model">accorderie.type.telephone</field>
        <field name="arch" type="xml">
            <graph string="Accorderie type telephone">
                <field name="nom" type="row" />
            </graph>
        </field>
    </record>

    <record id="accorderie_type_telephone_view_kanban" model="ir.ui.view">
        <field name="name">accorderie_type_telephone_kanban</field>
        <field name="model">accorderie.type.telephone</field>
        <field name="arch" type="xml">
            <kanban class="o_kanban_mobile">
                <field name="nom" />
                <field name="membre" />
                <field name="membre_2_ids" />
                <field name="membre_ids" />
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div class="oe_kanban_details">
                                <ul>
                                    <li class="mb4">
                                        <strong>
                                            <field name="nom" />
                                        </strong>
                                    </li>
                                    <li class="mb4">
                                        <strong>
                                            <field name="membre" />
                                        </strong>
                                    </li>
                                    <li class="mb4">
                                        <strong>
                                            <field name="membre_2_ids" />
                                        </strong>
                                    </li>
                                    <li class="mb4">
                                        <strong>
                                            <field name="membre_ids" />
                                        </strong>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <record id="accorderie_type_telephone_view_pivot" model="ir.ui.view">
        <field name="name">accorderie_type_telephone_pivot</field>
        <field name="model">accorderie.type.telephone</field>
        <field name="arch" type="xml">
            <pivot string="Accorderie type telephone">
                <field name="nom" type="row" />
            </pivot>
        </field>
    </record>

    <record id="accorderie_type_telephone_view_search" model="ir.ui.view">
        <field name="name">accorderie_type_telephone_search</field>
        <field name="model">accorderie.type.telephone</field>
        <field name="arch" type="xml">
            <search string="Accorderie type telephone">
                <filter name="nom" string="Nom" domain="[('nom','!=',False)]" />
                <filter name="membre" string="Membre" domain="[('membre','!=',False)]" />
                <filter name="membre_2_ids" string="Membre 2 Ids" domain="[('membre_2_ids','!=',False)]" />
                <filter name="membre_ids" string="Membre Ids" domain="[('membre_ids','!=',False)]" />
            </search>
        </field>
    </record>

    <record id="accorderie_type_telephone_view_tree" model="ir.ui.view">
        <field name="name">accorderie_type_telephone_tree</field>
        <field name="model">accorderie.type.telephone</field>
        <field name="arch" type="xml">
            <tree>
                <field name="nom" />
                <field name="membre" />
                <field name="membre_2_ids" />
                <field name="membre_ids" />
            </tree>
        </field>
    </record>

    <act_window
        id="accorderie_type_telephone_type_de_t&#233;l&#233;phones_action_window"
        name="Type de t&#233;l&#233;phones"
        res_model="accorderie.type.telephone"
        view_mode="kanban,pivot,list,graph,form"
    />
</odoo>
