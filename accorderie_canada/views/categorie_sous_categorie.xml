<?xml version= "1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="categorie_sous_categorie_treeview">
    <field name="name">categorie_sous_categorie_tree</field>
    <field name="model">categorie.sous.categorie</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="approuver"/>
        <field name="description"/>
        <field name="nocategorie"/>
        <field name="nocategoriesouscategorie"/>
        <field name="nooffre"/>
        <field name="nosouscategorie"/>
        <field name="supprimer"/>
        <field name="titreoffre"/>
      </tree>
    </field>
  </record>

  <record model="ir.actions.act_window" id="categorie_sous_categorie_categorie_sous_categorie_act_actionwindow">
    <field name="name">categorie_sous_categorie_action_view</field>
    <field name="res_model">categorie.sous.categorie</field>
  </record>

</odoo>