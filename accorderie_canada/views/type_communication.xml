<?xml version= "1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="type_communication_treeview">
    <field name="name">type_communication_tree</field>
    <field name="model">type.communication</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="notypecommunication"/>
        <field name="typecommunication"/>
