<?xml version= "1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="categorie_treeview">
    <field name="name">categorie_tree</field>
    <field name="model">categorie</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="approuver"/>
        <field name="nocategorie"/>
        <field name="supprimer"/>
        <field name="titrecategorie"/>
      </tree>
    </field>
  </record>

  <record model="ir.actions.act_window" id="categorie_categorie_action_view_actionwindow">
    <field name="name">categorie_action_view</field>
    <field name="res_model">categorie</field>
  </record>

</odoo>