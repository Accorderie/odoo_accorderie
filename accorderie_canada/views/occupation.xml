<?xml version= "1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="occupation_treeview">
    <field name="name">occupation_tree</field>
    <field name="model">occupation</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="nooccupation"/>
        <field name="occupation"/>
      </tree>
    </field>
  </record>

  <record model="ir.actions.act_window" id="occupation_occupation_action_view_actionwindow">
    <field name="name">occupation_action_view</field>
    <field name="res_model">occupation</field>
  </record>

</odoo>