<?xml version= "1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="produit_treeview">
    <field name="name">produit_tree</field>
    <field name="model">produit</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="datemaj_produit"/>
        <field name="noaccorderie"/>
        <field name="nomproduit"/>
        <field name="noproduit"/>
        <field name="notitre"/>
        <field name="taxablef"/>
        <field name="taxablep"/>
        <field name="visible_produit"/>
      </tree>
    </field>
  </record>

  <record model="ir.actions.act_window" id="produit_produit_action_view_actionwindow">
    <field name="name">produit_action_view</field>
    <field name="res_model">produit</field>
  </record>

</odoo>